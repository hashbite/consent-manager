{
  "name": "mono",
  "private": true,
  "devDependencies": {
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.10.3",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "cypress": "^5.2.0",
    "lerna": "^3.15.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.14.0",
    "typescript": "^3.9.7"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "prepublish": "lerna run prepublish",
    "start:demo:custom-theme": "yarn run build && yarn --cwd examples/cra-custom-theme && yarn --cwd examples/cra-custom-theme start",
    "start:demo:minimal": "yarn run build && yarn --cwd examples/parcel-minimal && yarn --cwd examples/parcel-minimal",
    "start:demo:minimal:lerna": "lerna run start --scope '@techboi/privacy-manager-example-parcel-minimal'",
    "start:demo:cra:lerna": "lerna run start --scope '@techboi/privacy-manager-example-cra'",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary"
  }
}
